package com.netfinworks.rest.resource.mock;

/**
 * @author bigknife
 *
 */
public class Order {
	private String id;
	private String name;
	private long amount;
	
	public Order() {
		super();
	}
	public Order(String id, String name, long amount) {
		super();
		this.id = id;
		this.name = name;
		this.amount = amount;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public long getAmount() {
		return amount;
	}
	public void setAmount(long amount) {
		this.amount = amount;
	}
	
}
